public class MainClass {

    public static void main(String[] args) {

        int a = 123456;
        int newNumber = 0;




        while(a>0){
            newNumber= newNumber*10+a%10;
            a = a/10;
            System.out.println("New Number: "+newNumber);
            System.out.println("Temp: "+a);
        }
    }
}
